{
  "compilerOptions": {
    "target": "es6",
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmitOnError": true,
    "jsx": "react",
    "declaration": true,
    "emitDeclarationOnly": true
  }
}
